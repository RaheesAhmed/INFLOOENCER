{
  "expo": {
    "name": "inflooencer",
    "slug": "inflooencer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/Splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.inflooncer"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Splash.png",
        "backgroundColor": "#ffffff",
        "resizeMode": "cover"
      },
      "package": "com.inflooncer"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
